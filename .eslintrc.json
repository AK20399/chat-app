{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "error",
        "no-console": "off",
        "no-underscore-dangle": "off"
    }
}
